# with literal string
format = """
$username\
$hostname\
$directory\
$git_branch\
$git_commit\
$git_state\
$git_status\
$aws\
$gcloud\
$env_var\
$status\
$cmd_duration\
$custom\
$line_break\
$jobs\
$battery\
$time\
$character"""

add_newline = false
scan_timeout = 10
#command_timeout = 100 # ms

[character]
format = "$symbol$symbol$symbol "

[directory]
truncation_length = 5
truncation_symbol = "â€¦/"

[git_branch]
format = "[$symbol$branch]($style) "
symbol = "ðŸŒ±"
#truncation_length = 26

[git_status]
disabled = true  # too late in monorepo

[aws]
disabled = true

[aws.region_aliases]
ap-northeast-1 = "tk"
us-east-1 = "va"

[line_break]
disabled = false

[status]
format = '[$symbol $status]($style) '
disabled = false

[time]
disabled = false
format = '[\[$time\]]($style) '
